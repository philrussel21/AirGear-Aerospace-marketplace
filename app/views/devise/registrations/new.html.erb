  <div class="text-m font-bold text-center my-2">
    <h1>Create your account</h1>
  </div>

  <div class="mb-4">
    <%= f.label :company_name, class: "block text-gray-700 text-sm font-bold mb-2"%>
    <%= f.text_field :company_name, autofocus: true, autocomplete: "company-name", class: "shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline", placeholder: "Company Name" %>
  </div>

  <div class="mb-4">
    <%= f.label :email, class: "block text-gray-700 text-sm font-bold mb-2" %>
    <%= f.email_field :email, autocomplete: "email", class: "shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline", placeholder: "Email Address"%>
  </div>

  <div class="mb-4">
    <%= f.label :password, class: "block text-gray-700 text-sm font-bold mb-2" %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %>
    <%= f.password_field :password, autocomplete: "new-password", class: "shadow appearance-none border border-red-500 rounded w-full py-2 px-3 text-gray-700 mb-3 leading-tight focus:outline-none focus:shadow-outline", placeholder: "******"%>
  </div>

  <div class="mb-4">
    <%= f.label :password_confirmation, class: "block text-gray-700 text-sm font-bold mb-2" %>
    <%= f.password_field :password_confirmation, autocomplete: "new-password", class: "shadow appearance-none border border-red-500 rounded w-full py-2 px-3 text-gray-700 mb-3 leading-tight focus:outline-none focus:shadow-outline", placeholder: "******" %>
  </div>

  <div class="actions flex justify-center">
    <%= f.submit "Sign up", class: "w-3/4 bg-primary hover:bg-hovered text-white font-bold py-2 px-4 rounded focus:outline-none focus:shadow-outline" %>
  </div>

  <div class="my-4">
  <hr>
  </div>

  <div class="text-sm font-bold text-center">
    <p>Already have an account?</p>
  </div>

  <div class="flex justify-center my-4">
    <button class="w-3/4 bg-primary hover:bg-hovered text-white font-bold py-2 px-4 rounded focus:outline-none focus:shadow-outline"><%= link_to 'Log in', new_account_session_path %></button>
  </div>
<% end %>
  <p class="text-center text-gray-500 text-xs">
    &copy;2020 Phil Antiporda
  </p>

</div>